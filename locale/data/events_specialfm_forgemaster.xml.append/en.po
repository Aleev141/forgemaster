msgstr "(Orchid) Your orchid already has a membership."

#: src-en/data/events_specialfm_forgemaster.xml.append:11592
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_QUEST_ARBOREAL\"]/choice/event/choice[@req=\"LIST_CREW_ORCHID\"]/event/text"
msgstr "\"Thank you. Enjoy your stay.\""

#: src-en/data/events_specialfm_forgemaster.xml.append:11594
msgid ""
