#: src-en/data/events_specialfm_forgemaster.xml.append:14188
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_COALITION_STORAGE_CRATES\"]/text"
msgstr "Select a Crate!"

#: src-en/data/events_specialfm_forgemaster.xml.append:14191
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_COALITION_STORAGE_CRATES\"]/choice[@req=\"marker_duct_tape\""
" and @lvl=\"1\"]/text"
msgstr ""
"You already retrieved all the duct tape you feel you'd ever need in your "
"entire life."

#: src-en/data/events_specialfm_forgemaster.xml.append:14196
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_COALITION_STORAGE_CRATES\"]/choice[@req=\"marker_duct_tape\""
" and @lvl=\"0\"]/text"
msgstr "Open a crate labelled \"Tape\"."

#: src-en/data/events_specialfm_forgemaster.xml.append:14201
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_COALITION_STORAGE_CRATES\"]/choice[@req=\"marker_fire_grenade_obtained\""
" and @lvl=\"1\"]/text"
msgstr ""
"You already retrieved all the Incendiary grenades you feel you'd need."

#: src-en/data/events_specialfm_forgemaster.xml.append:14206
